generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Alert {
  id                String   @id @default(uuid())
  timestamp         DateTime 
  regionCode        String
  city              String
  uaqi              Int
  pm25              Float
  pm10              Float
  indexesPayload    Json     
  createdAt         DateTime @default(now())
}